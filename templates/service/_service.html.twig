<div class="card mb-4">
    {% if service.picture %}
        <img src="{{ vich_uploader_asset(service, 'imageFile') | imagine_filter('thumb') }}" alt="card-img-top" style="width: 100%; height: auto">

    {% else %}
        <img src="{{ 'images/services/empty.jpg' | imagine_filter('thumb') }}" alt="card-img-top" style="width: 100%; height: auto">
    {% endif %}
    <div class="card-body">
        <h5 class="card-title">
            <a href="{{ path('service.show', {slug: service.slug, id: service.id}) }}">
                {{ service.title }}
            </a>
        </h5>
    </div>
</div>

