{{ form_start(form) }}
<div class="row">
    <div class="col-md-3">{{ form_row(form.title) }}</div>
    <div class="col-md-3">{{ form_row(form.description) }}</div>
    <div class="col-md-3">{{ form_row(form.link) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.imageFile) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}
